<div ng-if="process.AssociatedEligibleCost.ServiceLines.length === 0" class="margin-5px font-size--small">
	<p>{{process.ServiceCategoryDescription}}</p>
	<p class="text--bold">There are no {{process.ServiceCategoryCaption}} included in the project
	</p>
</div>
<div ng-if="process.AssociatedEligibleCost.ServiceLines.length > 0" class="margin-5px">
	<p>{{process.ServiceCategoryDescription}}</p>
	<div class="form--readonly">
		<div class="form__group">
			<div class="form__label">Total cost:</div>
			<div class="form__control">
				{{process.AssociatedEligibleCost.EstimatedCost | currency : '$' : 2}}
			</div>
		</div>
	</div>
</div>

